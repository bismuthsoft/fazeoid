{
	"name": "fmsite-svkit",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently -n web,audio npm:dev-web npm:dev-audio",
		"dev-web": "vite dev",
		"dev-audio": "esbuild --bundle --watch ./src/lib/soundgen/index.js --outfile=./static/soundgen.bundle.js",
		"build": "npm run build-web && npm run build-audio",
		"build-web": "vite build",
		"build-audio": "esbuild --bundle ./src/lib/soundgen/index.js --outfile=./static/soundgen.bundle.js",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@bismuthsoft/svelte-dj-knob": "^0.1.4",
		"@sveltejs/adapter-auto": "1.0.0-next.64",
		"@sveltejs/kit": "1.0.0-next.396",
		"@types/audioworklet": "^0.0.30",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"concurrently": "^7.3.0",
		"esbuild": "^0.14.54",
		"eslint": "^8.21.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-grommet-icons": "^0.2.1",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.4"
	},
	"type": "module"
}
